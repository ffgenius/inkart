{
  "name": "@inkart/cli",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.11.0",
  "description": "A Node-based scaffolding tool for quickly creating front-end project templates",
  "author": "青椒肉丝<https://github.com/pkc918>",
  "license": "MIT",
  "keywords": [
    "inkart",
    "cli",
    "node cli",
    "templates"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "create": "./index.mjs"
  },
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "scripts": {
    "build": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "prepack": "pnpm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@inkart/temps": "workspace:^",
    "cac": "catalog:prod",
    "chalk": "catalog:prod",
    "prompts": "catalog:prod"
  },
  "devDependencies": {
    "@rollup/plugin-json": "catalog:dev",
    "@types/prompts": "catalog:dev",
    "rollup": "catalog:dev",
    "rollup-plugin-esbuild": "catalog:dev",
    "ts-node": "catalog:dev",
    "typescript": "catalog:dev"
  }
}
